{
  "name": "@dotvirus/tunisia",
  "version": "0.4.2",
  "description": "Super simple, model-less DynamoDB wrapper",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "coverage": "nyc npm test",
    "postcoverage": "nyc report --reporter=json",
    "watch": "tsc -w",
    "test": "ava --verbose -c 1 test/**/*.spec.ts",
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --fix --ext .ts"
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marvin-j97/tunisia.git"
  },
  "keywords": [
    "dynamodb",
    "aws",
    "amazon",
    "dynamo",
    "query builder"
  ],
  "author": "marvin-j97",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marvin-j97/tunisia/issues"
  },
  "homepage": "https://github.com/marvin-j97/tunisia#readme",
  "peerDependencies": {
    "aws-sdk": ">=2.553.0"
  },
  "dependencies": {
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^8.1.0",
    "@types/debug": "^4.1.5",
    "@types/node": "^15.0.2",
    "ava": "^3.13.0",
    "aws-sdk": "^2.553.0",
    "nyc": "^15.1.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ]
  }
}
