{
  "name": "tunisia",
  "version": "1.0.0",
  "description": "Super simple, typesafe DynamoDB query builder",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "lint": "eslint \"**/*.ts\"",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "test": "vitest run --coverage"
  },
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marvin-j97/tunisia.git"
  },
  "keywords": [
    "dynamodb",
    "aws",
    "amazon",
    "dynamo",
    "query builder",
    "typesafe"
  ],
  "author": "marvin-j97",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marvin-j97/tunisia/issues"
  },
  "homepage": "https://github.com/marvin-j97/tunisia#readme",
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.316.0",
    "@aws-sdk/lib-dynamodb": "3.316.0"
  },
  "devDependencies": {
    "@types/node": "16.18.24",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "@vitest/coverage-istanbul": "0.26.3",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-jsdoc": "39.6.4",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "prettier": "2.8.1",
    "tsup": "6.5.0",
    "typescript": "4.9.4",
    "vitest": "0.26.3"
  }
}
